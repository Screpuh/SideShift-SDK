{
    "name": "sideshift-node-sdk",
    "version": "1.1.0",
    "description": "TypeScript Wrapper for SideShift.ai API",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "directories": {
        "example": "examples",
        "test": "tests"
    },
    "scripts": {
        "build": "tsc",
        "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
        "format": "prettier --write \"src/**/*.ts\"",
        "test": "jest",
        "prepare": "npm run build",
        "prepublishOnly": "npm test && npm run lint"
    },
    "keywords": [
        "api",
        "wrapper",
        "typescript",
        "sideshift"
    ],
    "author": "Screpuh",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/Screpuh/SideShift-SDK.git"
    },
    "devDependencies": {
        "@types/jest": "^29.5.14",
        "@types/node": "^22.7.6",
        "@typescript-eslint/eslint-plugin": "^8.9.0",
        "@typescript-eslint/parser": "^8.9.0",
        "eslint": "^9.12.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "jest": "^29.7.0",
        "prettier": "^3.3.3",
        "ts-jest": "^29.2.5",
        "ts-node": "^10.9.2",
        "typescript": "^5.6.3"
    }
}
